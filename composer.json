{
    "name": "sconcur/sconcur",
    "description": "Concurrency PHP via external demon",
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel",
            "email": "sprust28@gmail.com"
        }
    ],
    "require": {
        "ext-sockets": "*",
        "php": "^8.2",
        "psr/container": "^2.0",
        "psr/log": "^3.0",
        "mongodb/mongodb": "^1.21",
        "ext-mongodb": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "phpstan/phpstan": "^2.1",
        "symfony/var-dumper": "^7.3",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "SConcur\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SConcur\\Tests\\Feature\\": "tests/feature",
            "SConcur\\Tests\\Impl\\": "tests/impl"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
